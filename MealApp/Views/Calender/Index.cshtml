@{
}
@model IEnumerable<Models.Recipe>

<h1>
    Your Calender
</h1>
<body>
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>
                <th scope="col">Handle</th>
            </tr>
        </thead>
        <tbody>
           @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Name</th>
                </tr>
            }
        </tbody>
    </table>
</body>